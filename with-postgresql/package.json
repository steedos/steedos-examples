{
  "name": "@steedos-examples/with-postgresql",
  "version": "2.2.55-beta.4",
  "private": true,
  "scripts": {
    "start": "yarn down && yarn up && yarn start-pg  && yarn start-pgadmin && steedos start",
    "up": "docker-compose up -d",
    "down": "docker-compose down",
    "start-pg": "docker start postgresql",
    "stop-pg": "docker stop postgresql",
    "start-pgadmin": "docker start my-pgadmin",
    "stop-pgadmin": "docker stop my-pgadmin"
  },
  "dependencies": {
    "@steedos/service-enterprise": "2.2.55-beta.4"
  }
}
